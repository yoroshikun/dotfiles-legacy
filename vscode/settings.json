{
  "workbench.iconTheme": "material-icon-theme",
  "background.useFront": true,
  "background.useDefault": false,
  "background.customImages": [
    "https://pa1.narvii.com/6579/34a6f611e38a6a6888eb31f6375c080fd0784b28_128.gif"
  ],
  "background.style": {
    "content": "''",
    "pointer-events": "none",
    "position": "absolute",
    "z-index": "99999",
    "width": "100%",
    "height": "100%",
    "background-position": "100% 100%",
    "background-repeat": "no-repeat",
    "opacity": 0.25
  },
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.fontWeight": "600",
  "editor.tabSize": 2,
  "editor.cursorStyle": "line-thin",
  "files.trimTrailingWhitespace": true,
  "files.useExperimentalFileWatcher": true,
  "workbench.startupEditor": "none",
  "workbench.fontAliasing": "auto",
  "editor.fontSize": 13,
  "window.restoreFullscreen": true,
  "terminal.external.osxExec": "iTerm.app",
  "eslint.packageManager": "yarn",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "prettier.singleQuote": true,
  "background.enabled": false,
  "editor.formatOnSave": true,
  "explorer.confirmDelete": false,
  "flow.pathToFlow": "${workspaceRoot}/node_modules/.bin/flow",
  "window.zoomLevel": 0,
  "diffEditor.ignoreTrimWhitespace": true,
  "javascript.validate.enable": false,
  "workbench.colorTheme": "Over Night Owl",
  "flow.useNPMPackagedFlow": true,
  "prettier.jsxSingleQuote": true,
  "prettier.trailingComma": "all",
  "breadcrumbs.enabled": true,
  "editor.tokenColorCustomizations": {
    "[Over Night Owl]": {
      "textMateRules": [
        {
          "scope": "constant.language.boolean",
          "settings": {
            "foreground": "#e22dbe"
          }
        }
      ]
    }
  },
  "pgFormatter.keywordCase": "lowercase",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "mjml.lintEnable": false,
  "mjml.lintWhenTyping": false,
  "mjml.minifyHtmlOutput": false,
  "eslint.alwaysShowStatus": true
}
